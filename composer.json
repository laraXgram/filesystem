{
  "name": "laraxgram/filesystem",
  "description": "The LaraGram filesystem package.",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/laraxgram/core/issues",
    "source": "https://github.com/laraxgram/core"
  },
  "authors": [
    {
      "name": "LaraGram",
      "email": "laraxgram@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-hash": "*",
    "laraxgram/collections": "^3.0",
    "laraxgram/contracts": "^3.0",
    "laraxgram/macroable": "^3.0",
    "laraxgram/support": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "LaraGram\\Filesystem\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "suggest": {
    "ext-fileinfo": "Required to use the Filesystem class.",
    "ext-hash": "Required to use the Filesystem class."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}